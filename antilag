-- ======================
-- SE√á√ïES DENTRO DAS ABAS
-- ======================
local SectionHeadsizes = TabHeadsizes:CreateSection("Headsizes")
local SectionAimbot = TabAimbot:CreateSection("Aimbots")
local SectionESP = TabESP:CreateSection("Visualiza√ß√£o")
local SectionUtil = TabUtil:CreateSection("Utilit√°rios")

-- ======================
-- FUN√á√ÉO AUXILIAR PARA BOT√ïES
-- ======================
local function AddScriptButton(tab, section, name, linkOrFunc, isConfig, iconId)
    tab:CreateButton({
        Name = name,
        Callback = function()
            if typeof(linkOrFunc) == "string" then
                if isConfig then
                    loadstring(game:HttpGet(linkOrFunc))({
                        HideMenu = false,
                        Previous = 0
                    })
                else
                    loadstring(game:HttpGet(linkOrFunc))()
                end
            elseif typeof(linkOrFunc) == "function" then
                linkOrFunc()
            end
        end,
        Icon = iconId or 4483362458
    })
end

-- ======================
-- HEADSIZES
-- ======================
AddScriptButton(TabHeadsizes, SectionHeadsizes, "Headsize Haze", "https://pastebin.com/raw/y0uW2rbb", false)
AddScriptButton(TabHeadsizes, SectionHeadsizes, "Headsize BIB 1.50", "https://raw.githubusercontent.com/rlkbgdajogada/headsize/refs/heads/main/1.50", false)

